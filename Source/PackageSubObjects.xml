<gel:script
 xmlns:core="jelly:core"
    xmlns:gel="jelly:com.niku.union.gel.GELTagLibrary"
    xmlns:soap="jelly:com.niku.union.gel.SOAPTagLibrary"
    xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
    xmlns:soap-env="http://schemas.xmlsoap.org/soap/envelope/"
    xmlns:sql="jelly:sql"
    xmlns:xog="http://www.niku.com/xog"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:file="jelly:com.niku.union.gel.FileTagLibrary"
    xmlns:util="jelly:util"
    xmlns:q="http://www.niku.com/xog/Query">

	<core:set var="vFound">0</core:set>

	<core:set var="vStudioProjectID" value="${gel_objectInstanceId}"/>
	<core:if test="${DebugLevel &gt; 0}">
		<gel:log level="debug" message="Start Script"/>
		<gel:log level="debug" message=">>Studio Project: ${vStudioProjectID}"/>
		<gel:log level="debug" message=">>Persisted XOG URL  : ${XOGURL}"/>
		<gel:log level="debug" message=">>Persisted LOG Folder: ${XOGlogFolder}"/>
		<gel:log level="debug" message=">>Persisted SessionID: ${sessionID}"/>
		<gel:log level="debug" message=">>Persisted DebugLevel: ${DebugLevel}"/>
	</core:if>

    <gel:setDataSource dbId="Niku"/>

	<sql:query escapeText="false" var="vProject">
		select PACKAGING_PATH from odf_ca_stproj_project
		Where id = ${vStudioProjectID}
	</sql:query>
	<core:forEach items="${vProject.rowsByIndex}" var="row">
		<core:set var="vPackagingPath">${row[0]}</core:set>
		<core:if test="${DebugLevel &gt; 1}">
			<gel:log level="debug" message="vPackagingPath: ${vPackagingPath}"/>
		</core:if>
	</core:forEach>

	<!-- Fetching MasterObjects-->
	<sql:query escapeText="false" var="vElements">
		select distinct MASTER_OBJECT from odf_ca_stproj_element
		where element_type = 'SubObject'
		and odf_parent_id = ${vStudioProjectID}
	</sql:query>
	<core:forEach items="${vElements.rowsByIndex}" var="row">
		<core:set var="vMaster">${row[0]}</core:set>
		<core:if test="${DebugLevel &gt; 1}">
			<gel:log level="debug" message="vMaster   : ${vMaster}"/>
		</core:if>
		<core:set var="vFound">1</core:set>

		<!-- This is the XML Root Document-->
		<gel:parse var="v_xml_root">
			<NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_read.xsd">
				<Header version="8.0" action="read" objectType="contentPack" externalSource="NIKU">
					<args contentType="object" name="order_by_1" value="code"/>
					<args name="singleContentType" value="object"/>
					<args name="no_dependencies" value="true"/>
				</Header>
				<ObjectQuery>
					<Filter name="object_code" criteria="EQUALS">${vMaster}</Filter>
				</ObjectQuery>
			</NikuDataBus>
		</gel:parse>

		<core:if test="${vFound == 1}">
			<!-- Include this line for DEBUGGING only - it generates the "XML" file -->
			<core:if test="${DebugLevel &gt; 1}">
				<gel:serialize var="${v_xml_root}" fileName="${XOGlogFolder}/XOGREAD_03-SubObjects_${vMaster}_${vStudioProjectID}.xml"/>
			</core:if>

			<!-- Calling XOG-->
			<core:catch var="v_xog_exception">
				<soap:invoke endpoint="${XOGURL}/niku/xog" var="runresult">
					<soap:message>
						<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:xog="http://www.niku.com/xog">
							<soapenv:Header>
								<xog:Auth>
									<xog:SessionID>${sessionID}</xog:SessionID>
								</xog:Auth>
							</soapenv:Header>
							<soapenv:Body>
								<gel:include select="$v_xml_root/NikuDataBus"/>
							</soapenv:Body>
						</soapenv:Envelope>
					</soap:message>
				</soap:invoke>
			</core:catch>

			<!--Serializing Full Master Object for Debugging-->
			<core:if test="${DebugLevel &gt; 0}">
				<gel:serialize var="${runresult}" fileName="${XOGlogFolder}/XOGWRITE_03-SubObjects_Full_${vMaster}_${vStudioProjectID}.xml"/>
			</core:if>

			<!-- Generating new MasterObject XML -->
			<gel:parse var="v_xml_new">
				<NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
					<Header action="write" externalSource="NIKU" objectType="contentPack" version="8.0"/>
					<contentPack update="true">
						<objects>
						</objects>
					</contentPack>
				</NikuDataBus>
			</gel:parse>

			<gel:set var="vMIcode" select="$runresult/soapenv:Envelope/soapenv:Body/NikuDataBus/contentPack/objects/object/@code" asString="true"/>
			<gel:set var="vMIraiseEvents" select="$runresult/soapenv:Envelope/soapenv:Body/NikuDataBus/contentPack/objects/object/@raiseEvents" asString="true"/>
			<gel:set var="vMIsource" select="$runresult/soapenv:Envelope/soapenv:Body/NikuDataBus/contentPack/objects/object/@source" asString="true"/>

			<core:if test="${DebugLevel &gt; 1}">
				<gel:log level="debug" message="vMIcode: ${vMIcode}"/>
				<gel:log level="debug" message="vMIraiseEvents: ${vMIraiseEvents}"/>
				<gel:log level="debug" message="vMIsource: ${vMIsource}"/>
			</core:if>

			<gel:parse var="vMasterInstance">
				<object code="${vMIcode}" raiseEvents="${vMIraiseEvents}"
					source="${vMIsource}" update="true">
				</object>
			</gel:parse>

			<gel:forEach select="$runresult/soapenv:Envelope/soapenv:Body/NikuDataBus/contentPack/objects/object/nls" var="vNLS">
				<gel:set select="$vMasterInstance/object" value="${vNLS}" insert="true"/>
			</gel:forEach>

			<!-- Fetching SubObjects-->
			<sql:query escapeText="false" var="vElements2">
				select ELEMENT_ID from odf_ca_stproj_element
				where element_type = 'SubObject'
				and odf_parent_id = ${vStudioProjectID}
				and MASTER_OBJECT = '${vMaster}'
			</sql:query>
			<core:forEach items="${vElements2.rowsByIndex}" var="row">
				<core:set var="vElementID">${row[0]}</core:set>
				<core:if test="${DebugLevel &gt; 1}">
					<gel:log level="debug" message="vElementID   : ${vElementID}"/>
				</core:if>
				<core:set var="vFound">1</core:set>


				<gel:forEach select="$runresult/soapenv:Envelope/soapenv:Body/NikuDataBus/contentPack/objects/object/object" var="vSubObject">
					<gel:set var="vSubObjectCode" select="$vSubObject/@code" asString="true"/>
					<core:if test="${DebugLevel &gt; 1}">
						<gel:log level="debug" message="vSubObjectCode: ${vSubObjectCode}"/>
					</core:if>
					<core:if test="${vSubObjectCode == vElementID}">
						<gel:set select="$vMasterInstance/object" value="${vSubObject}" insert="true"/>
					</core:if>
				</gel:forEach>

			</core:forEach>

			<gel:set select="$v_xml_new/NikuDataBus/contentPack/objects" value="${vMasterInstance}" insert="true"/>

			<!--Generating Final OUTPUT -->
			<gel:serialize var="${v_xml_new}" fileName="${vPackagingPath}/03-SubObjects_${vMaster}.xml"/>
			<core:if test="${DebugLevel &gt; 0}">
				<gel:log level="debug" message="Final Output File: ${vPackagingPath}\03-SubObjects_${vMaster}.xml"/>
			</core:if>
		</core:if>
	</core:forEach>
</gel:script>
